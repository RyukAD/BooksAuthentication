<%- include("partials/header"); -%>

<p><% startingContent %></p>
<div class="add-books">
  <a href="/create" class="btn btn-success">Add more books?</a>
</div>

<% books.forEach(function(book){ %>
  <h1><%= book.title %></h1>
  <h4><%= book.author %></h4>
  <p><%= book.review.substring(0, 50)+"..." %><a href="books/<%=book._id%>">Read Full Review</a></p>

<%})%>

<%- include("partials/loggedInFooter"); %>
